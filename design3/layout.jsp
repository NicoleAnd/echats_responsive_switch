    <script type="text/javascript">
    	var index = $('.righthide').index($('.right.current'))+1;
	    function fanye(i){
	    	// 当前div .right索引
			
			// .right 个数
			var length = $('.righthide').length;
			if(i==0){
				//console.log(index);
				index--;
				index = (index)%length;
				if(index < length){     
					$('.righthide').eq(index).show().siblings().hide();
				}
			}else{
				//console.log(index);
				index++;
				index = index%length;
				if(index < length){     
					$('.righthide').eq(index).show().siblings().hide();
				}
			}
			$('.righthide').eq(index).show().siblings().hide();
	    }
		
		
